{
  "non-interactive": true,
  "dry-run": false,
  "verbose": false,
  "force": false,
  "pkgFiles": ["package.json"],
  "preReleaseId": "beta",
  "buildCommand": "ng build --prod",
  "requireCleanWorkingDir": false,
  "requireUpstream": false,
  "increment": "conventional:angular",
  "beforeChangelogCommand": "conventional-changelog -p angular -i CHANGELOG.md -s",
  "afterReleaseCommand": "echo Successfully released ${version} to ${dist.repo}.",
  "changelogCommand": "conventional-changelog -p angular | tail -n +3",
  "safeBump": true,
  "src": {
    "commit": true,
    "commitMessage": "Release %s",
    "commitArgs": "--no-verify",
    "tag": true,
    "tagName": "v%s",
    "tagAnnotation": "Release %s",
    "push": true,
    "pushArgs": "",
    "pushRepo": "origin master",
    "beforeStartCommand": false,
    "afterReleaseCommand": false,
    "addUntrackedFiles": false
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": true,
    "releaseName": "Release %s",
    "preRelease": true,
    "draft": false,
    "tokenRef": "GITHUB_TOKEN",
    "assets": null,
    "host": null,
    "timeout": 0
  },
  "dist": {
    "repo": "git@github.com:lokesh-coder/ink-bucket.git#gh-pages",
    "stageDir": ".stage",
    "baseDir": "dist/ink-bucket",
    "files": ["**/*"],
    "pkgFiles": null,
    "commit": true,
    "commitMessage": "Release %s",
    "commitArgs": "--no-verify",
    "tag": false,
    "tagName": "v%s",
    "tagAnnotation": "Release %s",
    "push": true,
    "pushArgs": "",
    "beforeStageCommand": false,
    "afterReleaseCommand": false,
    "addUntrackedFiles": false,
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  },
  "prompt": {
    "src": {
      "status": false,
      "commit": true,
      "tag": true,
      "push": true,
      "release": true,
      "publish": true
    },
    "dist": {
      "status": false,
      "commit": true,
      "tag": false,
      "push": true,
      "release": false,
      "publish": false
    }
  }
}

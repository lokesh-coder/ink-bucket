{
  "name": "ink-bucket",
  "version": "0.0.0-development",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "config:secrets": "ts-node ./config/env.secrets.ts",
    "start": "npm run config:secrets && ng serve --proxy-config config/proxy.conf.json -o --host=0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "server:prod": "http-server dist/ink-bucket -p 1234 -s -o --cors",
    "run:prod": "npm run build:prod && npm run server:prod",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "create:secret": "node ./config/create-secret"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/fire": "^5.1.0",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/material": "6.4.7",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/pwa": "0.10.5",
    "@angular/router": "6.1.10",
    "@angular/service-worker": "6.1.10",
    "@auth0/angular-jwt": "2.0.0",
    "@ctrl/tinycolor": "2.2.1",
    "@ngxs/devtools-plugin": "3.2.0",
    "@ngxs/store": "3.2.0",
    "a-color-picker": "1.0.6",
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "deep-freeze-strict": "1.1.1",
    "deepmerge": "2.1.1",
    "firebase": "^5.5.2",
    "fuse.js": "3.2.1",
    "lodash-es": "4.17.10",
    "ngx-clipboard": "11.1.3",
    "ngx-color": "2.0.3",
    "ngx-smart-modal": "6.0.4",
    "ngx-toastr": "8.10.0",
    "pretty-checkbox": "3.0.3",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.5",
    "@angular/cli": "6.2.7",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@semantic-release/changelog": "2.1.2",
    "@semantic-release/exec": "2.3.0",
    "@semantic-release/git": "6.0.2",
    "@semantic-release/github": "4.4.2",
    "conventional-changelog": "2.0.3",
    "dotenv": "6.1.0",
    "electron": "2.0.13",
    "husky": "0.14.3",
    "jest": "23.6.0",
    "jest-preset-angular": "5.2.3",
    "protractor": "5.4.0",
    "release-it": "7.5.0",
    "semantic-release": "15.9.8",
    "stylelint": "9.4.0",
